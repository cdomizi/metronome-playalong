{
  "name": "metronome-playalong",
  "version": "1.0.0",
  "main": "main.js",
  "type": "module",
  "packageManager": "yarn@4.5.3",
  "scripts": {
    "build": "yarn tsc",
    "lint:ts": "yarn tsc --noEmit",
    "lint": "eslint --report-unused-disable-directives --max-warnings 0 .",
    "dev": "tsup --watch --env.NODE_ENV development",
    "start": "tsup --env.NODE_ENV production"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.14.0",
    "prettier": "3.4.2",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.19.0"
  }
}
