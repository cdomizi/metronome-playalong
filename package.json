{
  "name": "metronome-playalong",
  "version": "1.0.0",
  "main": "./dist/main.js",
  "homepage": "https://cdomizi.github.io/metronome-playalong/",
  "type": "module",
  "packageManager": "yarn@4.6.0",
  "scripts": {
    "typecheck": "yarn tsc --noEmit",
    "lint": "eslint --report-unused-disable-directives --max-warnings 0 .",
    "build": "tsup --clean --env.NODE_ENV production",
    "dev": "tsup --watch --env.NODE_ENV development",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d dist"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@types/gh-pages": "^6.1.0",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "gh-pages": "^6.3.0",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "pinst": "^3.0.0",
    "prettier": "3.4.2",
    "tsup": "^8.3.5",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.21.0"
  }
}
